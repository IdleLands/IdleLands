{
  "name": "idlelands",
  "version": "4.0.0",
  "author": "Kyle Kemp <kyle@seiyria.com>",
  "homepage": "https://idle.land",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "node dist/out-tsc/server/server/index.js --inspect-workers",
    "start:client": "ng serve -o --host 0.0.0.0 --port 2468",
    "start:server": "ts-node --project src/server/tsconfig.server.json src/server/index.ts",
    "build:client": "ng build --prod",
    "build:server": "tsc --project src/server/tsconfig.server.json",
    "test": "npm run test:client && npm run test:server",
    "test:client": "npm run lint:client && npm run build:client",
    "test:server": "npm run lint:server && npm run build:server",
    "lint:client": "ng lint",
    "lint:server": "tslint --project src/server/tsconfig.server.json",
    "setup": "node scripts/post-install && npm run seed",
    "seed": "node scripts/seed",
    "dummyplayers": "node scripts/client/client",
    "combattest": "ts-node --project src/server/tsconfig.server.json scripts/combat-test.ts",
    "itemtest": "ts-node --project src/server/tsconfig.server.json scripts/item-test.ts",
    "debug:server": "TS_NODE_PROJECT=src/server/tsconfig.server.json node --inspect=0.0.0.0 -r ts-node/register src/server/index.ts"
  },
